    "lint:fix": "eslint --ext .ts . --fix",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.4",
    "rimraf": "^3.0.2",
    "slash-create": "^5.11.0",
    "slash-up": "^1.3.0",
    "ts-loader": "^7.0.5",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^3.3.12",
    "wrangler": "^2.12.0"
  },
  "dependencies": {
    "patch-package": "^6.5.1"
