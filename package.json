{
  "name": "slash-create-worker-template",
  "description": "A template for slash-create with Cloudflare Workers",
  "author": "Snazzah",
  "main": "dist/worker.js",
  "private": true,
  "scripts": {
    "build": "webpack",
    "dev": "wrangler dev -l",
    "deploy": "wrangler publish",
